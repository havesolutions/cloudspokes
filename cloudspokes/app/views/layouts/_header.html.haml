        or
        %a.trigger{:href => "#signup-popup"} sign up

  .cl &nbsp;
  #login-popup.popup.dvlogin
    .tab
      %span
    .t.notext
      \ 
    .cnt
      %fieldset{:style => "padding: 10px;"}
        %p
          Login below. Not a Member?
          = succeed "." do
            %a.signup-trigger{:href => "#signup-popup"}
              Sign up for
              free
          %span#errorMsg.error
        #ctl00_vsForgotPassword{:style => "color:Red;display:none;"}
        %p
        .field
