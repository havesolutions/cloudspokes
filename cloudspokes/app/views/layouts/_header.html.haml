.header
  %h1#logo.fl.notext= link_to "", "http://www.cloudspokes.com/default.html"

  #navigation.fl
    #logo2
    %div{:style => "clear: both"}
    %ul
      %li
        %a#ctl00_hypMembers{:href => "http://www.cloudspokes.com/members.html"}
          %span members
      %li
        %a#ctl00_hypChallenge{:href => "http://www.cloudspokes.com/Challenges.html"}
          %span challenges
      %li
        %a#ctl00_hypForum{:href => "http://www.cloudspokes.com/allbadges.html"}
          %span badges
      %li
        %a#ctl00_hypOverview{:href => "http://www.cloudspokes.com/project_overview.html"}
          %span account

  #utility-nav.fr
    %ul
      %li= link_to "how it works", "http://www.cloudspokes.com/howitworks.html"
      %li= link_to "blog", "http://blog.cloudspokes.com/"
      %li= link_to "faq", faq_path
      %li#ctl00_liLogin
